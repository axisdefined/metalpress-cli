{
  "name": "metalpress-cli",
  "version": "0.2.5",
  "description": "CLI for Metalpress (Official)",
  "license": "MIT",
  "main": "bin/metalpress.js",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.ava.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "build": "babel src -d lib",
    "test": "NODE_PATH=$NODE_PATH:$PWD/src ava",
    "test:watch": "npm t -- -w",
    "cover": "nyc npm t -r lcov",
    "report-coverage": "nyc report --reporter=text-lcov > coverage/coverage.lcov && codecov -t fd314356-ed0d-4f61-9588-48d3405179d6",
    "prepublish": "npm run build && npm test"
  },
  "bin": {
    "metalpress": "bin/metalpress.js",
    "metalpress-init": "bin/metalpress-init.js",
    "metalpress-new": "bin/metalpress-new.js",
    "metalpress-serve": "bin/metalpress-serve.js",
    "metalpress-deploy": "bin/metalpress-deploy.js"
  },
  "dependencies": {
    "bower-webpack-plugin": "0.1.9",
    "browser-sync": "2.18.2",
    "chalk": "1.1.3",
    "commander": "2.9.0",
    "deep-assign": "2.0.0",
    "denodeify": "1.2.1",
    "elegant-spinner": "^1.0.1",
    "figlet": "1.2.0",
    "fs-extra": "1.0.0",
    "github": "6.1.0",
    "gulp": "3.9.1",
    "gulp-s3": "0.3.0",
    "inquirer": "1.2.3",
    "js-yaml": "3.7.0",
    "jsonfile": "2.4.0",
    "log-update": "1.0.2",
    "ora": "^0.3.0",
    "prompt": "1.0.0",
    "resolve": "1.1.7",
    "shelljs": "0.7.5",
    "through": "2.3.8"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "codecov": "1.0.1",
    "metalpress": "0.8.1",
    "nyc": "10.0.0",
    "pmock": "0.2.3",
    "sinon": "1.17.6"
  }
}
